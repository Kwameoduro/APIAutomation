{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Album Schema",
  "type": "object",
  "required": ["userId", "id", "title"],
  "properties": {
    "userId": {
      "type": "integer",
      "minimum": 1
    },
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false
}
