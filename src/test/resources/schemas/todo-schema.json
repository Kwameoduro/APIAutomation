{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Single Todo Schema",
  "type": "object",
  "required": ["userId", "id", "title", "completed"],
  "properties": {
    "userId": {
      "type": "integer"
    },
    "id": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "completed": {
      "type": "boolean"
    }
  },
  "additionalProperties": false
}
